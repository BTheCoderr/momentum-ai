{
  "cli": {
    "version": ">= 5.9.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "distribution": "store",
      "node": "20.11.0",
      "env": {
        "APP_ENV": "production",
        "OFFLINE_MODE": "true"
      },
      "ios": {
        "resourceClass": "m-medium",
        "autoIncrement": true,
        "simulator": false
      },
      "android": {
        "autoIncrement": true
      }
    },
    "production-ios": {
      "extends": "production",
      "node": "20.11.0",
      "ios": {
        "resourceClass": "m-medium",
        "autoIncrement": true,
        "simulator": false,
        "buildConfiguration": "Release"
      }
    },
    "production-android": {
      "extends": "production",
      "android": {
        "autoIncrement": true
      }
    },
    "appstore": {
      "distribution": "store",
      "node": "20.11.0",
      "env": {
        "APP_ENV": "appstore",
        "OFFLINE_MODE": "true",
        "DEMO_MODE": "true"
      },
      "ios": {
        "resourceClass": "large",
        "autoIncrement": true,
        "simulator": false,
        "buildConfiguration": "Release"
      }
    },
    "production-ios-stable": {
      "distribution": "store",
      "node": "20.11.0",
      "env": {
        "APP_ENV": "production",
        "OFFLINE_MODE": "true"
      },
      "ios": {
        "resourceClass": "large",
        "autoIncrement": true,
        "simulator": false,
        "buildConfiguration": "Release"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6747629250",
        "appleTeamId": "2PF2FNX9X6"
      }
    },
    "production-ios-stable": {
      "ios": {
        "ascAppId": "6747629250",
        "appleTeamId": "2PF2FNX9X6"
      }
    }
  }
}
